<!DOCTYPE html>
<html>
<head>
  <title><%= full_title(yield(:title)) %></title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
<div class="wrapper">
<!-- place header here -->
			<% flash.each do |key, value| %>
				<div class="alert alert-<%= key %>">
					<p class="notice"><%= notice %></p>
		    	</div>
			<% end %>
			<div class="container">
				<%= yield %> 
			</div>
			<div class="push"></div>
			<!-- place footer here -->
		</div>
</body>
</html>
